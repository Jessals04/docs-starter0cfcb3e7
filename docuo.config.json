    "colorMode": {
      "defaultMode": "system",
      "disableSwitch": false
    },
    "navbar": {
      "title": "Lindisfarne Dev",
      "logo": {
        "light": "image/logo_light.png",
        "dark": "image/logo_dark.png"
      },
      "items": [
        {
          "type": "docSidebar",
          "sidebarId": "mySidebar",
