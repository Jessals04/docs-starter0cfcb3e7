        }
      ]
    },
    "footer": {
      "logo": {
        "light": "image/logo_light.png",
        "dark": "image/logo_dark.png"
      },
      "caption": "Lindisfarne Dev",
      "links": [
        {
          "title": "Products",
          "items": [
            {
              "label": "Lindisfarne AI",
              "href": "https://lindisfarne.ai"
            }
          ]
        }
      ],
      "socials": [
        {
          "logo": "https://www.lindisfarne.nsw.edu.au/wp-content/uploads/2021/04/LAGS_Crest_RGB_Digital-use.png",
          "href": "https://lindisfarne.nsw.edu.au"
        }
      ]
    }
  },
  "search": {
    "algolia": {
      "appId": "YOUR_APP_ID",
